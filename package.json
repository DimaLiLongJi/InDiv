{
  "name": "easiest",
  "version": "1.2.0",
  "description": "A minimal, blazing web mvvm framework.一个小的Web mvvm库。",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "src"
  ],
  "keywords": [
    "mvvm",
    "model",
    "view",
    "Class",
    "component",
    "front-end",
    "router",
    "DOM",
    "render",
    "framework",
    "mvc",
    "OOP",
    "Virtual DOM",
    "service"
  ],
  "scripts": {
    "start": "parcel ./demo/index.html -d dist --open",
    "start-ts": "parcel ./demo/index-ts.html -d dist --open",
    "build": "rm -rf build && parcel build ./src/index.ts --no-source-maps -d build && cp -r src/index.d.ts build",
    "pub-prod": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DimaLiLongJi/easiest.git"
  },
  "author": "DimaLiLongJi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DimaLiLongJi/easiest/issues"
  },
  "homepage": "https://github.com/DimaLiLongJi/easiest#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.0",
    "ejs": "^2.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.3",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.3",
    "nodemon": "^1.17.5",
    "parcel-bundler": "^1.8.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "easier-cookie": "^1.0.11",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
